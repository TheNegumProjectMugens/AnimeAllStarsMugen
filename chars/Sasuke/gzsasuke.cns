damage = 20,10
animtype = Light
guardflag = L
hitflag = MAF
priority = 3, Hit
pausetime = 10, 10
sparkno = S6014
guard.sparkno = S6016			;Spark anim no (Def: set above)
sparkxy = -5, -40
hitsound = S0, 7
guardsound = 6, 0
ground.type = High
ground.slidetime = 5
ground.hittime  = 12
